
@import "miniprogram_npm/vant-weapp/common/index.wxss";

/* 覆盖 vantUI */
.van-tag{
  margin: 0 10rpx 10rpx 0;
}
.van-tabbar--safe{
  padding-bottom: 0;
}
/* 覆盖weui */
.weui-loadmore__tips_in-line{
  top: 0;
}
.mini-btn{
  margin-right: 16rpx;
  font-size: 28rpx!important;
}
.weui-loadmore_line::after{
  content: "";
  position: absolute;
  width: 65%;
  left: 50%;
  height: 2rpx;
  background: rgba(0,0,0,.1);
  transform: translate(-50%,26rpx);
  z-index: -1;
}
.weui-loadmore_line{
  border: 0;
}
.weui-loadmore{
  margin: 1.5em auto 0;
  padding-bottom: 1em;
}
/* 覆盖weui e*/
.nobg{
  background: none
}
.color3{
  color: #333;
}
.color6{
  color: #666
}
.color9{
  color: #999;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  box-sizing: border-box;
}
.fl_col{
  display: flex;
  flex-direction: column
}
.fl_row_cen{
  display: flex;
  align-items: center;
}
.fl1{
  flex: 1;
}
.line{
  height: 2rpx;
  background: #ccc;
  width: 100%;
}
.linehei1{
  line-height: 1;
}

/* 综合排序第两行 */
.near_shop_filtrate_bar2 .van-button{
  margin-right: 16rpx;
  background: #F0F1E8;
  color: #898787;
  padding: 0 24rpx;
}

/* 店铺列表 */
.merchant_list{
  margin-top: 32rpx;
}
.merchant_item{
  display: flex;
  width: 100%;
  margin-bottom: 20rpx;
  /* margin-bottom: 60rpx; */
}
.merchant_item:last-child{
  margin-bottom: 0;
}
.merchant_item image{
  width: 188rpx;
  height: 160rpx;
  /* background: #999; */
  border-radius: 8rpx;
  margin-right: 12rpx;
}
.right_view{
  flex: 1;
  min-width: 0;
}
.right_view .smalltxt_line{
  font-size: 22rpx;
  position: relative;
  color: #999;
  height: 34rpx;
}
.right_view .txt_marg{
  margin-bottom: 4rpx;
}
.right_view .blue_txt{
  color: #67AFFB;
  margin-right: 12rpx;
}
.right_view .toright{
  position: absolute;
  right: 0;
}

/* .bg_tag_page{
  border: 2rpx solid #76B3F5;
  color: #76B3F5;
  padding: 2rpx 6rpx;
} */

/* 小哥豆明细 */
.all{
  height: 100%;
  width: 750rpx;
}
.all .all_item{
  height: 102rpx;
  border-bottom: 2rpx solid #FAFAFA;
  padding: 38rpx 32rpx 0 32rpx;
  position: relative;
}
.all .all_item text{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:#666666;
}
.all .all_item .type{
  display: block;
  color: #333333;
  font-size:28rpx;
}
.all .all_item .date{
  margin-right: 25rpx;
}
.all .all_item .money{
  position: absolute;
  right: 32rpx;
  top: 50%;
  transform: translateY(-50%);
}

/* 头部搜索框 */
.topbar .van-search{
  padding-left: 0;
  padding-right: 0;
}

/* 附近商家title */
.near_shop_title{
  font-weight: bold;
  font-size: 32rpx;
}

/* 我的团队 */
.team_item:nth-child(1) {
  margin-top: 12rpx;
}

.team_item {
  width: 750rpx;
  height: 100%;
  margin-top: 4rpx;
}

.team_item .teamItem {
  padding: 0 32rpx;
  height: 108rpx;
  background: rgba(255, 255, 255, 1);
  display: flex;
  justify-content: space-between;
}

.team_item .teamItem>view {
  width: 200rpx;
  float: left;
  height: 92rpx;
  padding-top: 16rpx;
}
.teamItem .item_left{
  width: 300rpx;
  flex: 1;
  position: relative;
}
.team_item .item_left image {
  width: 76rpx;
  height: 76rpx;
  background: rgba(232, 232, 232, 1);
  border-radius: 4rpx;
  float: left;
  margin-right: 16rpx;
}

.team_item .item_left text {
  position: absolute;
  left: 92rpx;  
  width: 100rpx;
  font-size: 24rpx;
  line-height: 17px;
  color: rgba(102, 102, 102, 1);
}
.team_item .item_left .shop_name{
  top:16rpx;
}
.team_item .item_left .level_name{
  bottom:16rpx;
}
.team_item .item_center view {
  font-size: 24rpx;
  text-align: center;
  color: rgba(51, 51, 51, 1);
}

.team_item .item_right {
  text-align: right;
  font-size: 22rpx;
  line-height: 70rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
}

.team_item .item_right .active {
  display: none;
}

.team_item .item_right image {
  margin-left: 18rpx;
  width: 22rpx;
  height: 12rpx;
}

/* 详细信息 */

.team_item .item_details {
  float: left;
  width: 718rpx;
  height: 250rpx;
  background: rgba(255, 255, 255, 1);
  padding: 16rpx 0rpx 0 32rpx;
  box-shadow: 0px 0px 8px rgba(91, 84, 84, 0.07);
  position: relative;
}

.team_item .item_details view {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  margin-bottom: 16rpx;
}

.team_item .item_details .look_details {
  position: absolute;
  right: 34rpx;
  top: 12rpx;
}

.team_item .item_details .look_details image {
  width: 12rpx;
  height: 22rpx;
  vertical-align: middle;
  margin-left: 18rpx;
}

.team_item .item_details .close_detail {
  width: 44rpx;
  height: 44rpx;
  position: absolute;
  left: 50%;
  bottom: 6rpx;
  transform:translateX(-50%);
  margin-bottom: 0;
  text-align: center;
}

.team_item .item_details .return {
  width: 22rpx;
  height: 12rpx;
  transform: rotate(180deg);
}

/* 新增收获地址和编辑收获地址 */
.address_detail{
  width: 750rpx;
  height: 696rpx;
}
.address_detail .item{
  width: 686rpx;
  height: 104rpx;
  padding: 0 32rpx;
  line-height: 104rpx;
  border-bottom: 2rpx solid rgba(249,251,245,1);
  position: relative;
}
.address_detail .item text{
  vertical-align: middle;
  color:rgba(102,102,102,1);
  font-size:28rpx;
  letter-spacing:2rpx;
}
.address_detail .item input{
  vertical-align: middle;
  display: inline-block;
  font-size: 24rpx;
}
.address_detail .item .ssq{
  width: 120rpx;
  text-align: center;
}
.address .choose{
  position: absolute;
  right: 32rpx;
  top: 50%;
  transform: translateY(-50%);
}
.address .choose image{
  width: 12rpx;
  height: 22rpx;
  vertical-align: middle;
  margin-left: 14rpx;
}
.address .choose text{
  vertical-align: middle;
  color:#CCCCCC;
  font-size: 22rpx;
}

.address_detail .item .default_btn{
  float: right;
  margin-top: 30rpx;
}

/* 商家大item */
.youxuanhot{
  margin: 0 20rpx 20rpx 20rpx;
  padding: 30rpx 20rpx;
  background: #fff;
  border-radius: 8rpx;
}
.youxuanhot .merchant_item{
  margin-bottom: 24rpx;
}
.youxuanhot .merchant_item image{
  width: 170rpx;
  height: 116rpx;
  margin-right: 22rpx;
}
.txt_marg_head{
  margin: 4rpx 0;
}
.merchant_title{
  flex: 1;
}
.hot_bottom{
  margin-top: 24rpx;
}
.bottom_img{
  width: 300rpx;
  height: 304rpx;
  background: #999;
  border-radius: 8rpx;
  margin-right: 22rpx;
}
.hot_right image{
  width: 350rpx;
  height: 144rpx;
  background: #999;
  border-radius: 8rpx;
}
.hot_right image:first-child{
  margin-bottom: 16rpx;
}

/* 外卖首页-为你优选 */
.youxuan_item {
  width: 334rpx;
  /* height: 314rpx; */
  display: flex;
  flex-direction: column;
  background: #f7f7f7;
  border-radius: 12rpx;
  position: relative;
}
.youxuan_item .manjian{
  position: absolute;
  top: 8rpx;
  right: 8rpx;
  text-align: center;
  width: 120rpx;
  height: 36rpx;
  line-height: 36rpx;
  background: #4B4848;
  border-radius: 18rpx;
  color: #fff;
  font-size: 22rpx;
}
.youxuan_item > image, .dis_top > image {
  width: 334rpx;
  height: 220rpx;
  background: #999;
  border-top-left-radius: 12rpx;
  border-top-right-radius: 12rpx;
}
.youxuan_item .youxuan_btxt {
  font-size: 30rpx;
  font-weight: bold;
}
.youxuan_btxt, .youxuan_sview {
  padding: 0 12rpx 0 20rpx;
}
.youxuan_item .youxuan_sview {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.youxuan_item .youxuan_sview .youxuan_stxt {
  flex: 1;
  font-size: 22rpx;
}

/* 搜索功能弹出层 */
.scroll_view{
  position: absolute;
  top: 106rpx;
  background: #fff;
  z-index: 100;
}
.nowaddress_item{
  padding: 28rpx 38rpx;
  height: 108rpx;
  box-sizing: border-box;
}
.searchview{
  transform: translate(-32rpx, 0);
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.nowaddress image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 8rpx;
}
.nowaddress image.down{
  width: 25rpx;
  height: 15rpx;
  margin-left: 8rpx;
}
.nowaddress .ser_fl1{
  flex: 1;
}
.nowaddress_item .van-tag{
  display: flex;
  margin-right:10rpx; 
}
.nowaddress_item .togetaddr{
  display: flex;
  align-items: center;
}
.nowaddress_item .search_item{
  display: flex;
  font-size: 28rpx;
}
.nowaddress_item .search_item .distance, .nowaddress_item .address{
  color: #999;
  font-size: 24rpx;
}
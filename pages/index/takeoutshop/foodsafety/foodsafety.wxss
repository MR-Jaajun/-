page {
  background: rgba(250, 250, 250, 1);
  display: flex;
  flex-direction: column;
  align-items: center;
}

.food_box {
  width: 710rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.05);
  border-radius: 4px;
  padding-top: 24rpx;
  margin-top: 24rpx;
}

.title {
  padding: 0 0 20rpx 20rpx;
}

.f_list view {
  font-size: 28rpx;
  color: #666;
  padding: 0 20rpx;
}

.f_list view.van-hairline--bottom {
  height: 88rpx;
  line-height: 88rpx;
}

.f_list image {
  width: 242rpx;
  height: 172rpx;
  background: rgba(216, 216, 216, 1);
  border-radius: 4rpx;
}
.f_img{
  display: flex;
  margin: 24rpx 0;
  justify-content: space-evenly;
}
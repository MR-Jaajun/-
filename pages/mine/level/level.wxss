page {
  width: 710rpx;
  padding: 0 20rpx;
  background:rgba(250,250,250,1);
}

.textcolor {
  color: #999;
  font-size: 28rpx;
  margin-top: 10rpx;
}

.equity {
  margin: 28rpx 0 16rpx 0;
  width: 670rpx;
  height: 142rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 0px 10rpx rgba(160, 157, 157, 0.04);
  border-radius: 28rpx;
  padding: 28rpx 20rpx 0 20rpx;
}
.equity view:nth-child(1){
  height:40rpx;
  font-size:28rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
  text-align: center;
  margin-bottom:28rpx;
}

.equity_info{
  width:202rpx;
  height:58rpx;
  background:rgba(243,243,243,1);
  border-radius:92px;
  float: left;
  font-size:24rpx;
  color:rgba(102,102,102,1);
  text-align: center;
  line-height: 58rpx;
  overflow: hidden;
}

/* 规则 */
.rule{
  width:574rpx;
  height:868rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 0px 10rpx rgba(160,157,157,0.04);
  border-radius:8rpx;
  position: relative;
  padding: 0 68rpx;
}
.rule>view:nth-child(1){
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 20rpx;
  font-size:32rpx;
  color:#333333;
}
.rule .rule_introduced{
 position: absolute;
 top: 62rpx;
}
.rule .rule_introduced>view{
  width: 574rpx;
  color: #666666;
  font-size: 24rpx;
}
page {
  background: rgba(250, 250, 250, 1);
  font-size: 28rpx;
  letter-spacing: 2rpx;
}
.title {
  width: 686rpx;
  height: 96rpx;
  padding: 0 32rpx;
  line-height: 96rpx;
  background: #fff;
  font-weight: bold;
  margin-bottom: 4rpx;
}
.title2{
  height: 114rpx;
  line-height: 114rpx;
  background: rgba(250, 250, 250, 1);
}
.problem_list {
  width: 686rpx;
  height: 96rpx;
  padding: 0 32rpx;
  background: #fff;
  margin-bottom: 6rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.problem_list text {
  color: rgba(51, 51, 51, 1);
}

.problem_list image {
  width: 12rpx;
  height: 22rpx;
}

/* 联系客服 */
.call{
  background: #fff;
  width: 750rpx;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.call .info{
  width: 200rpx;
  text-align: center;
  font-size: 24rpx;
  color:rgba(102,102,102,1);
}
.call image{
  width: 40rpx;
  height: 40rpx;
}

